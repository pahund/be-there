{
  "manifest_version": 3,
  "name": "Be there!",
  "version": "1.0",
  "description": "A simple Chrome extension to navigate to pre-defined URLs.",
  "permissions": ["tabs"],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "commands": {
    "open_url1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "MacCtrl+Shift+1"
      },
      "description": "GitHub Enterprise: open Searchlight pull requests"
    },
    "open_url2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "MacCtrl+Shift+2"
      },
      "description": "Jira: Searchlight spring board"
    },
    "open_url3": {
      "suggested_key": {
        "default": "Ctrl+Shift+3",
        "mac": "MacCtrl+Shift+3"
      },
      "description": "Jira: Searchlight user story map"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}

